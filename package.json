{
  "name": "fs_boilerplate-server",
  "author": "Anh Tuan Hoang",
  "description": "Express Typescript application boilerplate",
  "version": "0.1.0",
  "private": true,
  "homepage": "",
  "license": "",
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "resolutions": {
    "webpack": "^5"
  },
  "resolve": {
    "fallback": {
      "util": false
    }
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "dev:start": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "build": "rimraf dist && tsc --project tsconfig.json",
    "build:start": "yarn build && yarn start",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "pretty": "prettier --write .",
    "prepare": "husky install",
    "test": "jest -i --colors --verbose --forceExit",
    "sequelize": "node_modules/.bin/sequelize"
  },
  "dependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/method-override": "^0.0.32",
    "@types/morgan": "^1.9.4",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validation": "^4.1.0",
    "global": "^4.4.0",
    "helmet": "^6.0.1",
    "http-status": "^1.6.2",
    "joi": "^17.8.3",
    "jwt-simple": "^0.5.6",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.41",
    "morgan": "^1.10.0",
    "mysql2": "^3.2.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "sequelize": "^6.29.3",
    "socket.io": "^4.6.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@faker-js/faker": "^7.6.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "babel-register": "^6.26.0",
    "eslint": "^8.35.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "faker": "^6.6.6",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "nodemon": "^2.0.21",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
